{
  "manifest_version": 3,
  "name": "Time Rings",
  "description": "Extension clock that shows time in a creative way using p5.js",
  "version": "1.0",

  "icons": {
    "coffee": "icons/coffee.svg",
    "work": "icons/work.svg"
  },

  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Time Rings"
  },

  "permissions": [
    "storage"
  ]
}
